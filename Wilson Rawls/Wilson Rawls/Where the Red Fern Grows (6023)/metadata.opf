        <dc:title>Where the Red Fern Grows</dc:title>
        <dc:creator opf:file-as="Rawls, Wilson" opf:role="aut">Wilson Rawls</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (0.7.35) [http://calibre-ebook.com]</dc:contributor>
