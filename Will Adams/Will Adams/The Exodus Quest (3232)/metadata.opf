        <dc:title>The Exodus Quest</dc:title>
        <dc:creator opf:file-as="Adams, Will" opf:role="aut">Will Adams</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (0.7.35) [http://calibre-ebook.com]</dc:contributor>
